#include <stdio.h>

int main() {
    int nums[5] = {1,2,3,4,5};

    int newnums[5]; 
    int count = 5;

    for (int i = 0; i < 5; i++) {
        count = count - 1;
        newnums[i] = nums[count];
    }

    for (int i = 0; i < 5; i++) {
        printf("%d\n", nums[i]);
    }

    printf("\n\n");

    for (int i = 0; i < 5; i++) {
        printf("%d\n", newnums[i]);
    }

    return 0;
}
